macro(EXEC_CHECK CMD )
  execute_process(COMMAND ${CMD} ${ARGV1} ${ARGV2} ${ARGV3} ${ARGV4} ${ARGV5} ${ARGV6} ${ARGV7} ${ARGV8} ${ARGV9} ${ARGV10} RESULT_VARIABLE CMD_RESULT)
  message("CMD: ${CMD}")
  message("ARGV1 ${ARGV1}")
  message("ARGV2 ${ARGV2}")
  message("ARGV3 ${ARGV3}")
  message("ARGV4 ${ARGV4}")
  message("ARGV5 ${ARGV5}")
  message("ARGV6 ${ARGV6}")
  message("ARGV7 ${ARGV7}")
  message("ARGV8 ${ARGV8}")
  message("ARGV9 ${ARGV9}")
  message("ARGV10 ${ARGV10}")
  if(CMD_RESULT)
    message(FATAL_ERROR "Error running ${CMD}")
  endif()
endmacro()
exec_check(@TEST_TARGET@ --src-root @TEST_SROOT@ --doc-root @TEST_DROOT@ --toc --quiet @TEST_SOURCES@)
exec_check("@DIFF@" @TEST_OKROOT@ @TEST_DROOT@)